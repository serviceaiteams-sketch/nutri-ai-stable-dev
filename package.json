{
  "name": "nutriai-oracle",
  "version": "1.0.0",
  "description": "AI-Powered Nutrition Assistant - Refined Version",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
